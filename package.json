{
  "name": "fndr",
  "version": "0.1.0",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "npx prisma db seed",
    "prisma:generate": "npx prisma generate",
    "postinstall": "prisma generate",
    "testdb:setup": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "testdb:reseed": "dotenv -e .env.test -- npx prisma db seed",
    "dev:test": "dotenv -e .env.test -- next dev",
    "test:e2e:ui": "npx cypress open",
    "test:e2e:dev": "npm-run-all testdb:setup --parallel dev:test test:e2e:ui"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
    "@hookform/resolvers": "^3.2.0",
    "@next/font": "^13.4.13",
    "@prisma/client": "^5.1.1",
    "@types/node": "20.4.8",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.7",
    "@vercel/postgres": "^0.4.1",
    "axios": "^1.4.0",
    "csv-parser": "^3.0.0",
    "cuid": "^3.0.0",
    "cypress": "^13.0.0",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "luxon": "^3.4.0",
    "next": "13.4.13",
    "next-auth": "^4.22.5",
    "prisma": "^5.1.1",
    "react": "18.2.0",
    "react-datalist-field": "^20.3.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-icons": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.1",
    "dotenv-cli": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.1.1"
  }
}
